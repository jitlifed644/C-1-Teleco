#include <stdio.h>
#include <string.h>

int main() {
  char cadena[24];
  int longitud;
  
  fprintf (stdout,"\nDame una cadena: ");
  fscanf (stdin, "%s",cadena);

  longitud = strlen(cadena);
  if (longitud<=24 && longitud>=1){
    
     printf ("La cadena tiene %d caracteres\n\n", longitud);
  }
  else{
    printf ("Longitud excesiva");
  }
  return 0;
}
